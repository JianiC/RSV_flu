# this script contains param constraints of all the hypothesis 

# hypothesis neutral
neutral_param_constraints <- list(
  lower = c(R01 = 1, R02 = 1, 
            amplitude1 = 0, amplitude2 = 0, 
            tpeak1 = 0, tpeak2 = 0,
            rho1 = 0, rho2 = 0),
  upper = c(R01 = 10, R02 = 10, 
            amplitude1 = 1, amplitude2 = 1, 
            tpeak1 = 1, tpeak2 = 1,
            rho1 = 1, rho2 = 1)
)


